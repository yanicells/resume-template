{
  "rewrites": [
    {
      "source": "/",
      "destination": "/cv.pdf"
    }
  ],
  "headers": [
    {
      "source": "/cv.pdf",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/pdf"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
